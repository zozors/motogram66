<div class="index-main">
  <div class="album">
    <img class="img1" src="https://c0.wallpaperflare.com/preview/560/485/163/santa-monica-santa-monica-pier-united-states-route-66.jpg">
    <img class="img2" src="https://media.istockphoto.com/photos/historic-route-66-road-sign-picture-id529081140?k=20&m=529081140&s=612x612&w=0&h=rNWibgcZZQ8hV6hC5zY-ryGBX1NsgACf-5cw9jakmII=">
  </div>
</div>
<%= form_with(url: search_tweets_path, local: true, method: :get, class: "search-form") do |form| %>
  <%= form.text_field :keyword, placeholder: "投稿を検索する", class: "search-input keyframe5 animation" %>
  <%= form.submit "検索", class: "search-btn keyframe5 animation" %>
<% end %>
<div class="contents row ">
  <% @tweets.each do |tweet| %>
    <%= render partial: "tweet", locals: { tweet: tweet } %>
  <% end %>
</div>